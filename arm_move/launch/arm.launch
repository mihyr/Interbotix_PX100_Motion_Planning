<launch>
  <!--args-->
  <!--<arg name="rvizconfig" default="$(find arm_move)/rviz/arm.rviz" />
  <arg name="rviz" default="False"/>
  <arg name="URDF" default="$(find homework2)/urdf/turtlebot3_burger.urdf.xacro"/>-->

  <!--include-->
    <!--robot_model:=px100 use_actual:=true dof:=4-->
  <include file="$(find interbotix_xsarm_moveit)/launch/xsarm_moveit.launch">
  <!-- all vars that included.launch requires must be set -->
    <arg name="robot_model" value="px100" />
    <arg name="use_actual" value="true" />
    <arg name="dof" value="4" />
    <arg name="use_fake" value="false" />
    <arg name="use_gazebo" value="false" />
  </include>
  
  <!--load Figure 8 constants to ROS Parameter Server for nodes
  <rosparam command="load" file="$(find homework2)/config/trajectory.yaml" />-->
  <!--<param name="robot_description" command="$(find xacro)/xacro $(arg URDF)" />-->

    <!--nodes-->
  <node ns="px100" name="mover" pkg="arm_move" type="mover" output="screen">
      <param name="robot_name" value="px100"/>
      <param name="dof" value="4"/>
      <remap from="/attached_collision_object" to="/px100/attached_collision_object"/>
      <remap from="/collision_object" to="/px100/collision_object"/>
    <!--Pub Freq Parameter included in trajectory.yaml-->
  </node>

  
</launch>
